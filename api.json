          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "tokenId",
          "nftId",
          "owner",
          "quality",
          "multiLanguageTextMetadata",
          "image",
          "series",
          "count",
          "name",
          "description",
          "createdAt",
          "updatedAt"
        ]
      },
      "EnrichedERC721MetadataDtoImpl": {
        "type": "object",
        "properties": {
          "animation_url": {
            "type": "string"
          },
          "background_color": {
            "type": "string"
          },
          "image": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
        },
        "required": [
          "animation_url",
          "image",
          "name",
          "description"
        ]
      },
      "NFTDto": {
        "type": "object",
        "properties": {
          "nftId": {
            "type": "number"
          },
          "name": {
            "type": "number"
          },
          "quality": {
            "enum": [
              "SSR",
              "SR",
              "A",
              "B",
              "C"
            ],
            "type": "string"
          },
          "story": {
            "type": "number"
          },
          "recyclePrice": {
            "type": "number"
          },
          "currencyType": {
            "enum": [
              "BUSD",
              "ETH"
