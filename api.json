  "components": {
    "securitySchemes": {
      "x-api-key": {
        "type": "apiKey",
        "in": "header",
        "name": "x-api-key",
        "description": "API Key"
      }
    },
    "schemas": {
      "ERC721TextMetadataCls": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "description"
        ]
      },
      "MultiLanguageTextMetadataCls": {
        "type": "object",
        "properties": {
          "chinese": {
            "$ref": "#/components/schemas/ERC721TextMetadataCls"
          },
          "english": {
            "$ref": "#/components/schemas/ERC721TextMetadataCls"
          }
        }
      },
      "PutUserNFTDtoImpl": {
        "type": "object",
        "properties": {
          "tokenId": {
            "type": "number"
          },
          "nftId": {
            "type": "number"
          },
          "owner": {
            "type": "string"
          },
          "quality": {
            "enum": [
              "SSR",
              "SR",
              "A",
              "B",
              "C"
            ],
            "type": "string"
          },
          "series": {
            "type": "number"
          },
          "multiLanguageTextMetadata": {
            "$ref": "#/components/schemas/MultiLanguageTextMetadataCls"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          },
          "image": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
        },
        "required": [
          "tokenId",
          "nftId",
          "owner",
          "quality",
          "series",
          "multiLanguageTextMetadata",
          "createdAt",
          "updatedAt",
          "image",
          "name",
          "description"
        ]
      },
      "QueryParamCls": {
        "type": "object",
        "properties": {
          "value": {
            "type": "object"
          },
          "like": {
            "type": "boolean"
          }
        },
        "required": [
          "value"
        ]
      },
      "UserNFTQueryParamsCls": {
        "type": "object",
        "properties": {
          "owner": {
            "$ref": "#/components/schemas/QueryParamCls"
          }
        }
      },
      "UserNFTPaginatorCls": {
        "type": "object",
        "properties": {
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "updatedAt"
        ]
      },
      "UserNFTQueryDtoImpl": {
        "type": "object",
        "properties": {
          "queryParams": {
            "$ref": "#/components/schemas/UserNFTQueryParamsCls"
          },
          "paginator": {
            "$ref": "#/components/schemas/UserNFTPaginatorCls"
          },
          "limit": {
            "type": "number",
            "default": 100
          },
          "offset": {
            "type": "number",
            "default": 0
          }
        },
        "required": [
          "queryParams",
          "paginator"
        ]
