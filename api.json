{
  "openapi": "3.0.0",
  "info": {
    "title": "Open API",
    "description": "Open API for VSPark Backend Service",
    "version": "0.0.1",
    "contact": {}
  },
  "tags": [
    {
      "name": "vsparknft",
      "description": "VSPARK NFT相关服务"
    }
