            "type": "string"
          },
          "image": {
            "type": "string"
          },
          "series": {
            "type": "number"
          }
        },
        "required": [
          "nftId",
          "name",
          "quality",
          "story",
          "recyclePrice",
          "currencyType",
          "image",
          "series"
