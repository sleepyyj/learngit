      "SeriesDto": {
        "type": "object",
        "properties": {
          "seriesId": {
            "type": "number"
          },
          "seriesName": {
            "type": "number"
          },
          "nftIds": {
            "type": "array",
            "items": {
              "type": "number"
            }
          },
          "seriesPic": {
            "type": "string"
          },
          "allRecycleBonus": {
            "type": "number"
          }
        },
        "required": [
          "seriesId",
          "seriesName",
          "nftIds",
          "seriesPic",
          "allRecycleBonus"
        ]
      },
      "RecycleDto": {
        "type": "object",
        "properties": {
          "enable": {
            "type": "boolean"
          },
          "startRecycleNumber": {
            "type": "number"
          }
