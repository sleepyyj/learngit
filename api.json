        "required": [
          "enable",
          "startRecycleNumber"
        ]
      },
      "RecyclePriceDto": {
        "type": "object",
        "properties": {
          "quality": {
            "enum": [
              "SSR",
              "SR",
              "A",
              "B",
              "C"
