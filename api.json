  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "local"
    },
    {
      "url": "https://api-dev.vsparkverse.com",
      "description": "development"
    },
    {
      "url": "https://api.vsparkverse.com",
      "description": "production"
    }
